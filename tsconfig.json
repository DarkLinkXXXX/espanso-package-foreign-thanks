{
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "./dist",
    "target": "es5",
    "lib": ["es5"],
    "typeRoots": ["node_modules/@types"],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "sourceMap": false,
    "strict": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "include": ["src"]
}
